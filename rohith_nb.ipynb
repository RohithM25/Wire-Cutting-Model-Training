    "def accuracy(data, labels, weights):\n",
    "    outs = np.dot(data, weights)\n",
    "    binary = (outs >= 0.5).astype(int)\n",
    "\n",
    "    samples = data.shape[0]\n",
    "    correct = sum(binary == labels[:,0])\n",
    "    return correct/samples\n"
